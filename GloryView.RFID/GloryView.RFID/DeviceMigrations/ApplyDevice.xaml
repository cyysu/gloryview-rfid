<UserControl x:Class="GloryView.RFID.DeviceMigrations.ApplyDevice"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="700" x:Name="applyDevice">
    <Grid Background="#20000000">
        <Grid.RowDefinitions>
            <RowDefinition Height="44*" />
            <RowDefinition Height="416" />
            <RowDefinition Height="40*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="22*" />
            <ColumnDefinition Width="654" />
            <ColumnDefinition Width="24*" />
        </Grid.ColumnDefinitions>
        <Grid Grid.Column="1" Grid.Row="1">
            <Grid.Background>
                <ImageBrush ImageSource="/GloryView.RFID;component/Images/groud.jpg" />
            </Grid.Background>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="168" />
                <ColumnDefinition Width="486" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="40" />
                <RowDefinition Height="40" />
                <RowDefinition Height="40" />
                <RowDefinition Height="40" />
                <RowDefinition Height="40" />
                <RowDefinition Height="40" />
                <RowDefinition Height="40" />
                <RowDefinition Height="40" />
                <RowDefinition Height="40" />
                <RowDefinition Height="40" />
                <RowDefinition Height="16*" />
            </Grid.RowDefinitions>
            <Label Content="申请 32434234234" Height="39" HorizontalAlignment="Left" Name="label1" VerticalAlignment="Top" Width="650" FontSize="16" Grid.ColumnSpan="2" Margin="4,0,0,0">
                <Label.Background>
                    <ImageBrush ImageSource="/GloryView.RFID;component/Images/user/MBG.jpg" />
                </Label.Background>
            </Label>
            <Border BorderBrush="Silver" BorderThickness="1,1,1,1" Grid.Row="1">
                <Label Height="30" Name="label2" Width="151" FontSize="14" HorizontalContentAlignment="Right" Content="编 号" />
            </Border>
            <Border BorderBrush="Silver" BorderThickness="0,1,1,1" Grid.Column="1" Grid.Row="1">
                <Label Content="32434234234" Height="27" Name="label9" Width="474" FontSize="14" />
            </Border>
            <Border BorderBrush="Silver" BorderThickness="1,0,1,1" Grid.Row="2">
                <Label Content="名 称" Height="31" Name="label3" Width="151" FontSize="14" HorizontalContentAlignment="Right" />
            </Border>
            <Border BorderBrush="Silver" BorderThickness="0,0,1,1" Grid.Row="2" Grid.Column="1">
                <Label Content="IBM服务器" Height="27" Name="label10" Width="474" FontSize="14" />
            </Border>
            <Border BorderBrush="Silver" BorderThickness="1,0,1,1" Grid.Row="3">
                <Label Content="类 别" FontSize="14" Height="31" HorizontalContentAlignment="Right" Name="label4" Width="151" />
            </Border>
            <Border BorderBrush="Silver" BorderThickness="0,0,1,1" Grid.Column="1" Grid.Row="3">
                <Label Content="服务器" Height="27" Name="label11" Width="474" FontSize="14" />
            </Border>
            <Border BorderBrush="Silver" BorderThickness="1,0,1,1" Grid.Row="4">
                <Label Content="状 态" FontSize="14" Height="31" HorizontalContentAlignment="Right" Name="label5" Width="151" />
            </Border>
            <Border BorderBrush="Silver" BorderThickness="0,0,1,1" Grid.Column="1" Grid.Row="4">
                <Label Content="正常" Height="27" Name="label12" Width="474" FontSize="14" />
            </Border>
            <Border BorderBrush="Silver" BorderThickness="1,0,1,1" Grid.Row="5">
                <Label Content="所在机房" FontSize="14" Height="31" HorizontalContentAlignment="Right" Name="label6" Width="151" />
            </Border>
            <Border BorderBrush="Silver" BorderThickness="0,0,1,1" Grid.Column="1" Grid.Row="5">
                <Label Content="二号机房" Height="27" Name="label13" Width="474" FontSize="14" />
            </Border>
            <Border BorderBrush="Silver" BorderThickness="1,0,1,1" Grid.Row="6">
                <Label Content="申请类别" FontSize="14" Height="31" HorizontalContentAlignment="Right" Name="label7" Width="151" />
            </Border>
            <Border BorderBrush="Silver" BorderThickness="0,0,1,1" Grid.Column="1" Grid.Row="6">
                <Label Height="35" Name="label14" Width="474" />
            </Border>
            <Border BorderBrush="Silver" BorderThickness="1,0,1,1" Grid.Row="7">
                <Label Content="迁移到" Height="35" Name="label15" Width="151" HorizontalContentAlignment="Right" FontSize="14" />
            </Border>
            <Border BorderBrush="Silver" BorderThickness="0,0,1,1" Grid.Column="1" Grid.Row="7">
                <Label Height="35" Name="label16" Width="474" />
            </Border>
            <Button Content="关   闭" Grid.Column="1" Grid.Row="9" Height="33" HorizontalAlignment="Left" Name="button1" VerticalAlignment="Top" Width="91" Click="Close_applyDevice" Margin="241,6,0,0" />
            <Button Content="申   请" Grid.Row="9" Height="33" HorizontalAlignment="Left" Margin="9,6,0,0" Name="button2" VerticalAlignment="Top" Width="100" Grid.Column="1" />
            <ComboBox Grid.Column="1" Grid.Row="6" Height="24" HorizontalAlignment="Left" Margin="9,9,0,0" Name="comboBox1" VerticalAlignment="Top" Width="124" FontSize="11">
                <ComboBoxItem Content="请选择..." IsSelected="True" />
                <ComboBoxItem Content="报修" />
                <ComboBoxItem Content="迁移" />
                <ComboBoxItem Content="报废" />
            </ComboBox>
            <Label Content="描述信息" FontSize="14" Height="31" HorizontalContentAlignment="Right" Name="label8" Width="151" Margin="8,9,8,0" Grid.Row="8" />
            <TextBox Height="37" Name="textBox1" Width="477" TextWrapping="Wrap" FontSize="14" Grid.Column="1" Margin="3,2,6,0" Grid.Row="8" />
            <ComboBox Grid.Column="1" Grid.Row="7" Height="24" HorizontalAlignment="Left" Margin="9,7,0,0" Name="comboBox2" VerticalAlignment="Top" Width="127" FontSize="11">
                <ComboBoxItem Content="请选择..." IsSelected="True" />
                <ComboBoxItem Content="一号机房" />
                <ComboBoxItem Content="二号机房" />
                <ComboBoxItem Content="三号机房" />
            </ComboBox>
            <Border BorderBrush="Silver" BorderThickness="5" Height="416" HorizontalAlignment="Left" Name="border1" VerticalAlignment="Top" Width="654" Grid.RowSpan="11" Grid.ColumnSpan="2"></Border>
            <Border BorderBrush="Silver" BorderThickness="1,0,1,1" Grid.Row="8"  />
            <Border BorderBrush="Silver" BorderThickness="0,0,1,1" Grid.Column="1" Grid.Row="8"  />
        </Grid>
    </Grid>
</UserControl>
